                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">IF (SELECT COUNT(*) FROM TescaEDW.EDW.Fact_PurchaseAnalysis) &lt;=0
	SELECT p.TransactionID, p.TransactionNO, CONVERT(DATE, p.TransDate) as TransDate, Convert(DATE, OrderDate) OrderDate, CONVERT(DATE, DeliveryDate) DeliveryDate,
	DATEDIFF(DAY,OrderDate, p.DeliveryDate) +1 as DifferentialDays, p.VendorID, p.EmployeeID, p.ProductID, p.StoreID, p.Quantity, p.TaxAmount, p.LineAmount, GETDATE() as LoadDate
	FROM PurchaseTransaction p WHERE CONVERT(DATE, p.TransDate) &lt;= DATEADD(DAY, -1, CONVERT(DATE, GETDATE())) 
ELSE
	SELECT p.TransactionID, p.TransactionNO, CONVERT(DATE, p.TransDate) as TransDate, Convert(DATE, OrderDate) OrderDate, CONVERT(DATE, DeliveryDate) DeliveryDate,
	DATEDIFF(DAY,OrderDate, p.DeliveryDate) +1 as DifferentialDays, p.VendorID, p.EmployeeID, p.ProductID, p.StoreID, p.Quantity, p.TaxAmount, p.LineAmount, GETDATE() as LoadDate
	FROM PurchaseTransaction p WHERE CONVERT(DATE, p.TransDate) = DATEADD(DAY, -1, CONVERT(DATE, GETDATE()))</property>
